
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/Layout.cshtml";
}



    <div class="page-content">
        <div class="container-fluid">

            <!-- start page title -->
            <div class="row">
                <div class="col-12">
                    <div class="page-title-box d-flex align-items-center justify-content-between">
                        <h4 class="mb-0 font-size-18">Dashboard</h4>

                    </div>
                </div>
            </div>

        @await Component.InvokeAsync("DashboardStatisticVC")
        

      
        <div class="row">
            
            @await Component.InvokeAsync("DashboardLineChartVC")
            @await Component.InvokeAsync("DashboardPieChartVC")
            @await Component.InvokeAsync("DashboardBarChartVC")
        </div>


            <div class="row">
            @await Component.InvokeAsync("DashboardContactMessagesVC")


            @await Component.InvokeAsync("DashboardServiceListVC")
                
            </div>

        </div> 
    </div>


   


<script src="~/Admin/vertical/assets/js/jquery.min.js"></script>
<script src="~/Admin/vertical/assets/js/bootstrap.bundle.min.js"></script>
<script src="~/Admin/vertical/assets/js/metismenu.min.js"></script>
